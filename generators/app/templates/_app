(function (angular) {
  // Controllers

  /**
   * @ngdoc controller
   * @name dummyController
   * @namespace dummyController
   * @description
   *    Dummy Controller.
   */
  function dummyController() {

    // Assign controller to `vm`
    var vm = this;

    // Parameters

    vm.test = 'Hello';

  }

  // Setup controllers
  angular.module('dummyApp.controllers', [])
    .controller('dummyController', dummyController);

  // Setup everyhting
  angular.module('dummyApp', [
    'dummyApp.controllers',
  ]);

})(angular);
